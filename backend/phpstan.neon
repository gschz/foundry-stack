includes:
    - vendor/larastan/larastan/extension.neon
    - vendor/nesbot/carbon/extension.neon

parameters:

    phpVersion: 80408

    paths:
        - app
        - bootstrap/
        - config
        - Modules
        - ../database
        - public
        - routes

    excludePaths:
        - bootstrap/cache/

    level: max

    tmpDir: bootstrap/cache/phpstan